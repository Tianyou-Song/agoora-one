{
	"exclude": [
		".md"
	],
	"formatter": [
		"progress-bar",
		{
			"minCount": 10
		}
	],
	"ignore": [
		"**/.nyc_output",
		"**/coverage",
		"**/dist*",
		"**/fixture",
		"**/node_modules",
		"**/package-lock.json"
	],
	"parser": "babel",
	"plugins": [
		"add-return-await",
		"apply-destructuring",
		"apply-nullish-coalescing",
		"apply-numeric-separators",
		"apply-optional-chaining",
		"apply-shorthand-properties",
		"apply-top-level-await",
		"convert-apply-to-spread",
		"convert-arguments-to-rest",
		"convert-array-copy-to-slice",
		"convert-binary-expression-to-boolean",
		"convert-commonjs-to-esm",
		"convert-equal-to-strict-equal",
		"convert-esm-to-commonjs",
		"convert-for-each-to-for-of",
		"convert-for-in-to-for-of",
		"convert-for-to-for-of",
		"convert-generic-to-shorthand",
		"convert-index-of-to-includes",
		"convert-math-pow",
		"convert-object-assign-to-merge-spread",
		"convert-spread-to-array-from",
		"convert-template-to-string",
		"convert-throw",
		"convert-to-arrow-function",
		"convert-top-level-return",
		"extract-object-properties",
		"extract-sequence-expressions",
		"merge-destructuring-properties",
		"merge-if-statements",
		"promises",
		"react-hooks",
		"regexp",
		"remove-boolean-from-logical-expressions",
		"remove-console",
		"remove-constant-conditions",
		"remove-debugger",
		"remove-double-negations",
		"remove-duplicate-interface-keys",
		"remove-duplicate-keys",
		"remove-empty",
		"remove-empty-pattern",
		"remove-iife",
		"remove-nested-blocks",
		"remove-only",
		"remove-process-exit",
		"remove-skip",
		"remove-unreachable-code",
		"remove-unreferenced-variables",
		"remove-unused-expressions",
		"remove-unused-for-of-variables",
		"remove-unused-private-fields",
		"remove-unused-types",
		"remove-unused-variables",
		"remove-useless-arguments",
		"remove-useless-array-from",
		"remove-useless-async",
		"remove-useless-await",
		"remove-useless-escape",
		"remove-useless-for-of",
		"remove-useless-functions",
		"remove-useless-spread",
		"remove-useless-template-expressions",
		"remove-useless-type-convertion",
		"remove-useless-typeof",
		"remove-useless-types",
		"remove-useless-variables",
		"reuse-duplicate-init",
		"simplify-ternary",
		"split-nested-destructuring",
		"split-variable-declarations",
		"strict-mode",
		"webpack"
	],
	"processors": [
		"css",
		"ignore",
		"javascript",
		"json",
		"markdown",
		"yaml"
	],
	"rules": {
		"add-return-await": true,
		"apply-destructuring/array": true,
		"apply-destructuring/object": true,
		"apply-nullish-coalescing": true,
		"apply-numeric-separators": true,
		"apply-optional-chaining": true,
		"apply-shorthand-properties": [
			true,
			{
				"ignore": []
			}
		],
		"apply-top-level-await": true,
		"convert-array-copy-to-slice": true,
		"convert-binary-expression-to-boolean": true,
		"convert-commonjs-to-esm": false,
		"convert-equal-to-strict-equal": true,
		"convert-esm-to-commonjs": false,
		"convert-for-each-to-for-of": true,
		"convert-for-in-to-for-of/negative": true,
		"convert-for-in-to-for-of/positive": true,
		"convert-for-to-for-of/length": true,
		"convert-for-to-for-of/n": true,
		"convert-generic-to-shorthand": true,
		"convert-index-of-to-includes": true,
		"convert-math-pow": true,
		"convert-object-assign-to-merge-spread": true,
		"convert-spread-to-array-from": true,
		"convert-template-to-string": true,
		"convert-to-arrow-function": true,
		"convert-top-level-return": true,
		"extract-object-properties/equal-deep": true,
		"extract-object-properties/not-equal-deep": true,
		"extract-sequence-expressions": true,
		"merge-destructuring-properties": true,
		"merge-if-statements": true,
		"promises/add-return-await": true,
		"promises/convert-reject-to-throw": true,
		"promises/remove-useless-resolve": true,
		"react-hooks/convert-class-to-function": true,
		"react-hooks/convert-component-to-use-state": true,
		"react-hooks/convert-import-component-to-use-state": true,
		"react-hooks/convert-state-to-hooks": true,
		"react-hooks/remove-bind": true,
		"react-hooks/remove-this": true,
		"react-hooks/rename-method-under-score": true,
		"regexp/convert-replace-to-replace-all": true,
		"regexp/convert-to-string": true,
		"regexp/optimize": true,
		"regexp/simplify": true,
		"remove-boolean-from-logical-expressions": true,
		"remove-console": true,
		"remove-constant-conditions": true,
		"remove-debugger": true,
		"remove-double-negations": true,
		"remove-duplicate-interface-keys": true,
		"remove-duplicate-keys": true,
		"remove-empty-pattern": true,
		"remove-empty/argument": true,
		"remove-empty/block": true,
		"remove-empty/import": false,
		"remove-empty/pattern": true,
		"remove-iife": true,
		"remove-nested-blocks": true,
		"remove-only": true,
		"remove-process-exit": true,
		"remove-skip": true,
		"remove-unreachable-code": true,
		"remove-unreferenced-variables": true,
		"remove-unused-expressions": true,
		"remove-unused-for-of-variables": true,
		"remove-unused-private-fields": true,
		"remove-unused-types": true,
		"remove-unused-variables": true,
		"remove-useless-arguments/arguments": true,
		"remove-useless-arguments/destructuring": true,
		"remove-useless-array-from": true,
		"remove-useless-async": true,
		"remove-useless-await": true,
		"remove-useless-escape": true,
		"remove-useless-for-of": true,
		"remove-useless-functions": true,
		"remove-useless-spread/array": true,
		"remove-useless-spread/object": true,
		"remove-useless-template-expressions": true,
		"remove-useless-typeof": true,
		"remove-useless-types": true,
		"remove-useless-variables/await": true,
		"remove-useless-variables/for-of": [
			true,
			{
				"maxProperties": 4
			}
		],
		"remove-useless-variables/remove": true,
		"remove-useless-variables/rename": true,
		"reuse-duplicate-init": true,
		"simplify-ternary": true,
		"split-nested-destructuring": true,
		"split-variable-declarations": true,
		"strict-mode/add": false,
		"strict-mode/remove": true,
		"webpack/convert-loader-to-use": true
	}
}